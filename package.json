{
  "type": "module",
  "name": "@denshya/proton-demo",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "dev": "vite",
    "dev:profile": "vite --profile",
    "build": "vite build",
    "preview": "vite preview",
    "stats": "bun stats:treemap && bun stats:sunburst",
    "stats:treemap": "vite-bundle-visualizer --sourcemap -o ./stats/treemap.html",
    "stats:sunburst": "vite-bundle-visualizer -o ./stats/sunburst.html -t sunburst",
    "stats:network": "vite-bundle-visualizer -o ./stats/network.html -t network"
  },
  "dependencies": {
    "@denshya/flow": "^0.3.0",
    "@denshya/proton": "^0.2.0",
    "urlpattern-polyfill": "^10.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "eslint": "9.16.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "sass": "^1.82.0",
    "type-fest": "^4.30.0",
    "typescript": "^5.7.2",
    "vite": "^6.0.7",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "engines": {
    "bun": "1.1.45"
  }
}